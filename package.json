  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "braces": "^3.0.2",
    "eslint": "^6.8.0",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "handlebars": "^4.5.3",
    "if-env": "^1.0.4",
    "js-yaml": "^3.13.1",
    "merge": "^2.1.1",
    "mime": "^2.4.4",
    "mongoose": "^5.8.4",
    "webpack-dev-server": "^3.10.1"
  }
